#include <FastLED.h>

#define LED_PIN     27
#define COLOR_ORDER BRG
#define CHIPSET     WS2812
#define NUM_LEDS    256

#define BRIGHTNESS  255
#define FRAMES_PER_SECOND 60

bool gReverseDirection = false;

CRGB leds[NUM_LEDS];

struct FONT_INFO {
  int charHeight;
  int startChar;
  int endChar;
  int spacePixelWidth;
  const void* descriptors;
  const byte* bitmaps;
 
};

extern const FONT_INFO constantia_11ptFontInfo;

void setup() {
  delay(3000);

  Serial.begin(115200);
  
  FastLED.addLeds<CHIPSET, LED_PIN, COLOR_ORDER>(leds, NUM_LEDS); //.setCorrection( TypicalLEDStrip );
  FastLED.setBrightness( BRIGHTNESS );
}

void setRowLeds(int row, CRGB color) {
  int ledix = 8*row;
  for(int i = 0; i < 8; i++) {
    leds[ledix++] = color;
  }
  FastLED.show();
}

void loop() {
  int w = renderChar(constantia_11ptFontInfo, 0, 0, '8');
  renderChar(constantia_11ptFontInfo, w, 0, '1');
  FastLED.show();
}

void setLed(int x, int y, CRGB color) {
  if(x < 0 || x >= 16 || y < 0 || y >= 16) 
    return;

  int panelRowIndex = x % 8;
  int xPanelSelect = ((~x >> 3) & 0x1);          // 0 = panel 1, 1 - panel 0

  int apx = (7 - panelRowIndex) + xPanelSelect * 64;
  if(y >= 8) {
    y -= 8;
    apx += 128;
  }

  apx += y * 8;
  if(apx < 0 || apx > NUM_LEDS) {
    Serial.println("offset error");
    return;
  }

  leds[apx] = color;

}


int renderChar(FONT_INFO fi, int xoff, int yoff, char c) {
  if(c < fi.startChar || c > fi.endChar)
    c = '?';
  
  int cix = c - fi.startChar;
  int* spec = (int*) fi.descriptors;
  spec += 2 * cix;                    // Point to width byte
  int cWidth = *spec++;
  int cOff = *spec;

  char buf[80];
  sprintf(buf, "cw=%d, coff=%d\n", cWidth, cOff);
  Serial.println(buf);

  const byte* cptr = fi.bitmaps + cOff;
  for(int cy = 0; cy < fi.charHeight; cy++) {
    byte data = *cptr++;
    for(int cx = 0; cx < cWidth; cx++) {
      int pel = data & 0x80;
      data = data << 1;
      setLed(xoff + cx, 15 - (yoff + cy), pel == 0 ? CRGB::Black : CRGB::White);
    }
  }

  return xoff + cWidth + fi.spacePixelWidth;
}

